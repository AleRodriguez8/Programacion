package app;

import java.sql.Time;
import java.time.LocalDate;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;

public class prueba2 {
	public static void main(String [] args) {
		ArrayList <Chat> mnsj = new ArrayList <>();
		Time hora = Time.valueOf("19:00:00");
		String nombre = "pepe";
		String msj = "";
		LocalDate fe1 = LocalDate.of(2021, 10, 1);
		LocalDate fe2 = LocalDate.of(2021, 5, 1);
		LocalDate fe3 = LocalDate.of(2021, 5, 15);
		LocalDate fe4 = LocalDate.of(2022, 1, 5);
		Chat f1 = new Chat(fe1,hora,nombre,msj);
		Chat f2 = new Chat(fe2,hora,nombre,msj);
		Chat f3 = new Chat(fe3,hora,nombre,msj);
		Chat f4 = new Chat(fe4,hora,nombre,msj);
		mnsj.add(f1);
		mnsj.add(f2);
		mnsj.add(f3);
		mnsj.add(f4);
		Collections.sort(mnsj);
		//for(Chat aux: mnsj) {
			//System.out.println(aux);
		//}
	}
}
